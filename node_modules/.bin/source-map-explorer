#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../_source-map-explorer@1.4.0@source-map-explorer/index.js" "$@"
  ret=$?
else 
  node  "$basedir/../_source-map-explorer@1.4.0@source-map-explorer/index.js" "$@"
  ret=$?
fi
exit $ret
